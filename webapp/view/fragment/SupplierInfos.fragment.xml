<core:FragmentDefinition
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core">
    <m:VBox id="_IDGenVBox1">
        <m:HBox id="_IDGenHBox19">
            <m:Label id="_IDGenLabel23" text="Supplier ID : " labelFor="_IDGenText20" />
            <m:Text id="_IDGenText24" text="{SupplierID}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox2">
            <m:Label id="_IDGenLabel" text="Supplier Name : " labelFor="_IDGenText20" />
            <m:Text id="_IDGenText20" text="{ToSupplier/CompanyName}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox4">
            <m:Label id="_IDGenLabel12" text="Email : "  labelFor="_IDGenText16" />
            <m:Text id="_IDGenText16" text="{ToSupplier/EmailAddress}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox5">
            <m:Label id="_IDGenLabel13" text="City : " labelFor="_IDGenText17" />
            <m:Text id="_IDGenText17" text="{ToSupplier/Address/City}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox6">
            <m:Label id="_IDGenLabel14" text="Postal Code : " labelFor="_IDGenText18" />
            <m:Text id="_IDGenText18" text="{ToSupplier/Address/PostalCode}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox7">
            <m:Label id="_IDGenLabel15" text="Street : " labelFor="_IDGenText19" />
            <m:Text id="_IDGenText19" text="{ToSupplier/Address/Street}" />
        </m:HBox>
        <m:HBox id="_IDGenHBox8">
            <m:Label id="_IDGenLabel16" text="Country : " labelFor="_IDGenText21" />
            <m:Text id="_IDGenText21" text="{ToSupplier/Address/Country}" />
        </m:HBox>
        
    </m:VBox>
</core:FragmentDefinition>